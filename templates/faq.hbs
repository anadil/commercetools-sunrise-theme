<!DOCTYPE html>
<html lang="en">

<head>
  {{> common/htmlhead}}
</head>

<body>
  <div class="darkbg hidden"></div>
  {{> common/header}}

  <div class="container">
    <div class="faq-title">{{i18n "faq.title"}}</div>
    <div class="faq-wrapper">
      <div class="faq-section-title">{{i18n "faq.delivery"}}</div>
      {{#each content.deliveryFaqs}}
        {{> catalog/faq-accordion}}
      {{/each}}
      <div class="faq-section-title">{{i18n "faq.returns"}}</div>
      {{#each content.returnsFaqs}}
        {{> catalog/faq-accordion}}
      {{/each}}
    </div>
  </div>

  {{> common/footer}}
  {{> common/htmlscripts}}
</body>

</html>
